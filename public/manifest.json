{
  "name": "Philosophie Group Chrome Extension",
  "version": "0.0.1",
  "description": "This Chrome extension greets Philosophers each time they open a new tab",
  "manifest_version": 2,
  "incognito": "split",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Philosophie"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["activeTab"],
  "icons": {
    "16": "./img/favicon-16x16.png",
    "32": "./img/favicon-32x32.png",
    "128": "./img/favicon-128x128.png"
  }
}
